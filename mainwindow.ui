    <property name="styleSheet">
     <string notr="true">/* Style pour le QTabWidget */
QTabWidget#tabWidget {
    /* Rendre le fond du QTabWidget semi-transparent */
    background-color: rgba(255, 255, 255, 0.3); /* Blanc avec 30% d'opacité */
}

/* Style pour la barre d'onglets (QTabBar) */
QTabWidget#tabWidget::tab-bar {
    alignment: center; /* Centrer les onglets */
}

/* Style pour chaque onglet dans la barre d'onglets */
QTabBar::tab {
    background-color: rgba(255, 255, 255, 0.5); /* Fond des onglets semi-transparent */
    color: #000000; /* Texte noir pour lisibilité */
    padding: 5px;
    margin: 2px;
    border: 1px solid #0078D7; /* Bordure bleue pour contraste */
    border-radius: 5px;
}

/* Style pour l'onglet sélectionné */
QTabBar::tab:selected {
    background-color: rgba(255, 255, 255, 0.8); /* Fond plus opaque pour l'onglet actif */
    color: #000000;
    border: 1px solid #005BB5; /* Bordure plus foncée pour l'onglet actif */
}

/* Style pour le contenu des onglets (pane) */
QTabWidget#tabWidget::pane {
    background-color: rgba(255, 255, 255, 0.3); /* Fond du contenu semi-transparent */
    border: 1px solid #0078D7; /* Bordure bleue */
    border-radius: 5px;
}</string>
